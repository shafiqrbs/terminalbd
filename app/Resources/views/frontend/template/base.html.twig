<!DOCTYPE html>
<html lang="en">
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!--> <html lang="en" class="no-js"> <!--<![endif]-->
<head>
    {% set currentPath = path(app.request.attributes.get('_route'),
    app.request.attributes.get('_route_params')) %}

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0"/>
    <meta name="apple-mobile-web-app-capable" content="yes"/>
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
   {# <meta property="og:url"           content="htpp://www.{{ globalOption.domain }}{{ currentPath }}" />
    <meta property="og:type"          content="website" />
    <meta property="og:title"         content="{{ globalOption.name }}" />
    <meta property="og:description"   content="{{ globalOption.name }}" />
    <meta property="og:image"         content="{{ image(globalOption.templateCustomize.webPath('logo')).resize() }}" />
#}

    <meta property="og:title" content="{{ globalOption.name }}" />
    <meta property="og:url" content="http://{{ globalOption.domain }}" />
    <meta property="og:image" content="{{ image(globalOption.templateCustomize.webPath('logo')).resize() }}" />
    <meta property="og:description" content="{{ globalOption.name }}" />
    <meta property="og:site_name" content="{{ globalOption.name }}" />
    <title>{{ globalOption.name }}</title>
    {% if globalOption.templateCustomize.favicon is not empty %}
    <link  rel="shortcut icon" href="/uploads/domain/{{ globalOption.id }}/customizeTemplate/{{ globalOption.templateCustomize.favicon }}">
    {% endif %}

    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyDmior4xLqsriysUeQhtdSawc8yjDyaWaw"></script>
    <script type="text/javascript" src="https://www.google.com/recaptcha/api.js"></script>
    <!-- Google fonts - witch you want to use - (rest you can just remove) -->
    <link rel='stylesheet' href='http://fonts.googleapis.com/css?family=Open+Sans:400,800,700italic,700,600italic,600,400italic,300italic,300|Roboto:100,300,400,500,700&amp;subset=latin,latin-ext' type='text/css' />
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <!-- HEADER -->
    <!-- HEADER -->
    {% set template = globalOption.siteSetting.theme.folderName|lower %}
    {#{% set csstemplate = "assets/template/desktop/{{ template }}/css/style.css" %}#}
    {% stylesheets filter='cssrewrite'
    'assets/template/desktop/css/bootstrap.min.css'
    'assets/template/desktop/font-awesome/css/font-awesome.min.css'
    'assets/template/plugin/owl.carousel/owl.carousel.min.css'
    'assets/template/plugin/owl.carousel/owl.theme.min.css'
    'assets/template/plugin/owl.carousel/owl.transitions.css'
    'assets/template/plugin/side.toolbar/vcore.sidebar.css'
    'assets/template/plugin/pgslider/pgwslider.min.css'
    'assets/template/plugin/revolution_slider/css/rs-style.css'
    'assets/template/plugin/revolution_slider/rs-plugin/css/settings.css'
    'assets/template/plugin/textillate/assets/animate.css'
    'assets/template/plugin/wow/css/animate.css'
    'assets/template/plugin/select2/select2.css'
    'assets/plugins/pop-modal/popModal.min.css'
    'assets/template/plugin/range-slider/bootstrap-slider.css'
    'assets/template/desktop/css/module.css'
    %}

    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
      <link href="/assets/template/desktop/{{ template }}/css/style.css" rel="stylesheet" />

    <!--[if lt IE 9]>

    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    {% block cssBlock %}{% endblock %}
</head>
<body data-spy="scroll">
{{ include('frontend/template/templateCustomize.html.twig')}}
<!-- Navigation -->
{% render controller("FrontendBundle:TemplateWidget:header",{'globalOption':globalOption, 'menu':menu.id}) %}
<div class="clearfix"></div>
{% block body %}{% endblock %}
{% if globalOption.templateCustomize.footerBlock == 1 %}
{{ include('@Frontend/Template/Desktop/'~globalOption.siteSetting.theme.folderName~'/footer.html.twig') }}
{% endif %}
<footer id="footer">
     <div class="container">
        <div class="row myfooter">
            <div class="col-sm-6">
                <div class="pull-left">
                    &copy; Copyright {{ globalOption.name }} {{ "now"|date('Y') }}
                </div>
            </div>
            <div class="col-sm-6">
                <div class="pull-right">Designed & Developed by <a target="_blank" href="http://www.terminalbd.com">{{ portal_design_development }}</a></div>
            </div>
        </div>
    </div>
</footer>
<div id="myModal" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" >
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content"></div>
    </div>
</div>
<div id="product-modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" >
    <div class="modal-dialog modal-lg" role="document">
        <div class="product-modal-content modal-content"></div>
    </div>
</div>
<div id="email-confirm" style="display:none; text-align: left!important;">Thank you for your interest in {{ globalOption.domain }}. Your email has been sent successfully to our customer service department.</div>
<div id="email-invalid" style="display:none; text-align: left!important;">Dear Customer, All ready your email/mobile has been send. Thank you for your interest in {{ globalOption.domain }}.</div>
<div id="product-confirm" style="display:none; text-align: left!important;">Thank you for your interest in {{ globalOption.domain }}. Your order has been sent successfully to our customer service department.</div>
{% render controller("FrontendBundle:TemplateWidget:modalLogin",{'globalOption':globalOption.id}) %}
{% if globalOption.templateCustomize.showSidebar == 1 %}
    <div class="side-toolbar" style=""></div>
{% endif %}
<!-- jQuery -->
<!--Jquery Smooth Scrolling-->
{% javascripts

'assets/template/desktop/js/jquery.js'
'assets/template/desktop/js/bootstrap.min.js'
'assets/template/plugin/jquery-inputmask/jquery.inputmask.bundle.min.js'
'assets/template/plugin/numeric/jquery.numeric.js'
'assets/template/plugin/validation/jquery.validate-1.14.0.min.js'
'assets/template/plugin/validation/jquery-validate.bootstrap-tooltip.min.js'
'assets/template/plugin/owl.carousel/owl.carousel.min.js'
'assets/template/plugin/select2/select2.js'
'assets/template/plugin/wow/js/wow.min.js'
'assets/template/plugin/treeview/treeview.js'
'assets/template/plugin/matchHeight/height-match.js'
'assets/template/plugin/side.toolbar/vcore.sidebar.js'
'assets/template/plugin/range-slider/bootstrap-slider.min.js'
'assets/template/plugin/textillate/assets/jquery.fittext.js'
'assets/template/plugin/textillate/assets/jquery.lettering.js'
'assets/template/desktop/js/jquery.scrollbar.min.js'
'assets/template/plugin/jquery-zoom/zoom.js'
'assets/plugins/pop-modal/popModal.js'
'assets/template/desktop/js/scripts.js'

%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>  subdomain = "{{ globalOption.subDomain }}"; </script>
{% block javascriptBlock %}{% endblock %}
<script type="text/javascript">
$(document).ready(function(){
    {% block documentready %}{% endblock %}
});
</script>
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'UA-111963509-1');
</script>
</body>
</html>
