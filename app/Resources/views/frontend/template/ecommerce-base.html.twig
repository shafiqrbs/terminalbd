</div>
{% render controller("FrontendBundle:TemplateWidget:modalLogin",{'globalOption':globalOption.id}) %}
{% if globalOption.templateCustomize.showSidebar == 1 %}
    <div class="side-toolbar" style=""></div>
{% endif %}
<!-- jQuery -->
<!--Jquery Smooth Scrolling-->
{% javascripts

'assets/template/desktop/js/jquery.js'
'assets/template/desktop/js/bootstrap.min.js'
'assets/template/plugin/jquery-inputmask/jquery.inputmask.bundle.min.js'
'assets/template/plugin/numeric/jquery.numeric.js'
'assets/template/plugin/validation/jquery.validate-1.14.0.min.js'
'assets/template/plugin/validation/jquery-validate.bootstrap-tooltip.min.js'
'assets/template/plugin/owl.carousel/owl.carousel.min.js'
'assets/template/plugin/select2/select2.js'
'assets/template/plugin/wow/js/wow.min.js'
'assets/template/plugin/treeview/treeview.js'
'assets/template/plugin/matchHeight/height-match.js'
'assets/template/plugin/side.toolbar/vcore.sidebar.js'
'assets/template/plugin/range-slider/bootstrap-slider.min.js'
'assets/template/plugin/textillate/assets/jquery.fittext.js'
'assets/template/plugin/textillate/assets/jquery.lettering.js'
'assets/template/desktop/js/jquery.scrollbar.min.js'
'assets/template/plugin/jquery-zoom/zoom.js'
'assets/template/desktop/js/scripts.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>

{% endjavascripts %}
<script>  subdomain = "{{ globalOption.subDomain }}"; </script>
{% block javascriptBlock %}{% endblock %}
<script>

    wow = new WOW(
        {
            boxClass:     'wow',
            animateClass: 'animated',
            offset:       100
        }
    );
    wow.init();

    $(document).ready(function(){

        var navbar = $('.navbar-header').outerHeight(true);
        var mins = 22;
        $('#header-form').css({
            'margin-top':(navbar/2 - mins)
        });

/*        var navbarHeader = $('.navbarHeader').outerHeight(true);
        var mins = 22;
        $('#header-form').css({
            'margin-top':(navbarHeader/2 - mins)
        });*/

        $(".numeric").numeric();
        $(".mobile").inputmask("mask", {"mask": "99999-999-999"});

        var stickyOffset = $('.sticky').offset().top;
        $(window).scroll(function(){
            var sticky = $('.sticky'),
                scroll = $(window).scrollTop();
            if (scroll >= stickyOffset) sticky.addClass('fixed');
            else sticky.removeClass('fixed');
        });

        $('.social-tooltip').tooltip({
            selector: "[data-toggle=tooltip]",
            container: "body"
        });

        {% if globalOption.templateCustomize.sidebarTooltip == 1 %}

        $('.side-toolbar').vSidebar({
            'items': [
                {% render controller("FrontendBundle:TemplateWidget:sidebar",{'globalOption':globalOption.id}) %}
            ],
            'position': 'right bottom'
        });

        {% endif %}

        $(".dropdown").hover(
            function() {
                $('.dropdown-menu', this).not('.in .dropdown-menu').stop(true,true).slideDown("400");
                $(this).toggleClass('open');
            },
            function() {
                $('.dropdown-menu', this).not('.in .dropdown-menu').stop(true,true).slideUp("400");
                $(this).toggleClass('open');
            }
        );
        $('#list').click(function(event){event.preventDefault();$('#product .item').addClass('list-group-item');});
        $('#grid').click(function(event){event.preventDefault();$('#product .item').removeClass('list-group-item');
        $('#products .item').addClass('grid-group-item');});

        $('body').append('<div id="toTop" class="btn btn-primary color1"><span class="glyphicon glyphicon-chevron-up"></span></div>');
        $(window).scroll(function () {
            if ($(this).scrollTop() != 0) {
                $('#toTop').fadeIn();
            } else {
                $('#toTop').fadeOut();
            }
        });
        $('#toTop').click(function(){
            $("html, body").animate({ scrollTop: 0 }, 700);
            return false;
        });

        $("#price-range").slider({
            tooltip: 'always'
        });
        {% block documentready %}{% endblock %}

    });

</script>
