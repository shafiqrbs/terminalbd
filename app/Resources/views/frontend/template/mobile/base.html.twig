<!DOCTYPE html>
<html lang="en">
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!--> <html lang="en" class="no-js"> <!--<![endif]-->

<head>
    <meta charset="utf-8"/>
    <title>{{ globalOption.name }}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport"/>
    <meta content="" name="description"/>
    <meta content="" name="author"/>
    <meta name="MobileOptimized" content="320">
    <meta property="og:site_name" content="{{ globalOption.name }}">
    <meta property="og:title" content="{{ globalOption.name }}">
    <meta property="og:description" content="-CUSTOMER VALUE-">
    <meta property="og:type" content="website">
    <meta property="og:image" content="{{ image(globalOption.templateCustomize.webPath('logo')).resize() }}"><!-- link to image for socio -->
    <meta property="og:url" content="{{ globalOption.domain }}">
    <link rel="shortcut icon" href="{{ asset('assets/img/favicon.ico') }}">
    <script type="application/x-javascript"> addEventListener("load", function() {setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    {% stylesheets filter='cssrewrite'
    'assets/template/mobile/css/bootstrap.min.css'
    'assets/template/mobile/css/font-awesome.min.css'
    'assets/template/mobile/css/jquery.mmenu.all.css'
    'assets/template/mobile/css/style.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
    <!-- web-fonts -->
    <link href='//fonts.googleapis.com/css?family=Abril+Fatface' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
    <!-- //web-fonts -->
    {{ include('frontend/template/mobile/templateCustomize.html.twig')}}
</head>
<body >
{% block cssBlock %}{% endblock %}
<div class="body-back siteBgColor">
    <div class="masthead pdng-stn1">
        <div class="phone-box wrap push" id="home">
            <div class="menu-notify">
                <div class="profile-left">
                    <a href="#mm-menu" class="main-menu"><i class="fa fa-navicon"></i></a>
                </div>

                <div class="Profile-mid">
                    <h5 class="pro-link"><a class="site-name" href="{{ path('mobile_home',{'subdomain':globalOption.subDomain}) }}">{{ globalOption.name }}</a></h5>
                </div>
                <div class="clearfix"></div>
            </div>
            <!-- banner -->
            <div class="details-grid">
                <div class="details-shade">
                    <div class="details-right">
                       {% if globalOption.templateCustomize.webPath('logo') is not empty %} <img src="{{ image(globalOption.templateCustomize.webPath('logo')).resize(120) }}" alt=" ">{% endif %}
                        <h3>Welcome</h3>
                     </div>
                </div>

            </div>
            <!-- //banner -->
            {% block body %}{% endblock %}
            <!--/footer-->
            <div id="side-bar-panel" class="left-shadow-overlay" >
                <div class="handle">
                    <span class="glyphicon glyphicon-chevron-right"></span>
                </div>
                <div class="menu-notify box-link">
                    <div class="text-left">{{ app.user }}</div>
                    <ul class="social-links">
                        <li><a href="javascript:" class="register-btn"><i class="fa fa-sign-in"></i></a></li>
                        <li><a href="javascript:" class="login-preview"><i class="fa fa-lock"></i></a></li>
                        <li><a href="javascript:" class="cart-preview"><i class="fa fa-shopping-cart"></i></a></li>
                    </ul>
                </div>
                <div id="cart-details">
                    <div class="item-list" style="background: #222">
                        {% if  cart.contents is not empty %}
                            <ul class="cart-list">
                                {% for item in cart.contents %}
                                    <li id="item-remove-{{ item.rowid }}">
                                        <div class="item">
                                            <div class="col-md-12 cart-product-title">{{ item.name }}</div>
                                            <div class="col-md-6">
                                                {% if item.productImg is not empty %}
                                                    <img src="{{ item.productImg }}" height="100" width="160" alt=""/>
                                                {% endif %}
                                            </div>
                                            <div class="col-md-6">
                                                <div class="input-group">
                                                    <span class="input-group-addon">{{ globalOption.ecommerceConfig.currency }}&nbsp;{{ item.price }}</span>
                                                    <input type="text" class="form-control" width="80" id="{{ item.rowid }}" value="{{ item.quantity }}">
                                                </div><!-- /input-group -->
                                                <div class="btn-group text-right col-md-12" role="group" >
                                                    <button id="{{ item.rowid }}"  data-url="{{ path('webservice_product_cart_remove',{'cartid':item.rowid}) }}"
                                                            class="btn  btn-danger pull-right hunger-remove-cart" data-id="{{ item.price }}" data-value="{{ item.quantity }}" ><span class="glyphicon glyphicon-trash"></span>
                                                    </button>
                                                    <button id="{{ item.rowid }}"  data-url="{{ path('webservice_product_cart_update',{'cartid':item.rowid}) }}"
                                                            class="btn btn-success pull-right hunger-update-cart"><span class="glyphicon glyphicon-pencil"></span>
                                                    </button>

                                                </div>
                                            </div>
                                        </div>
                                   </li>
                                {% endfor %}
                            </ul>
                            <div class="text-center col-md-12"><a class="btn btn-warning " href="javascript:">Grand Total<b class="totalAmount">{{ globalOption.ecommerceConfig.currency }}&nbsp;{{ cart.total }}</b></a><a class="btn btn-info" href="{{ path('webservice_product_cart_details') }}">View Cart</a></div>
                        {% endif %}
                    </div>
                </div>
                <div id="login-signip">
                    {% render controller("FrontendBundle:TemplateWidget:modalLogin",{'globalOption':globalOption.id}) %}
                </div>
            </div>
            <div class="w3agile">
                <!-- modal -->
                <div id="email-modal" class="mfp-hide">
                    <div class="login-modal">
                        <div class="booking-info">
                            <h3><a href="{{ path('mobile_home',{'subdomain':globalOption.subDomain}) }}">{{ globalOption.name }}</a></h3>
                        </div>
                        <div class="login-form">
                            <form action="{{ path('webservice_email_receive',{'subdomain':globalOption.subDomain}) }}" id="send-email" name="send-email" method="post">
                                <div class="input-group ">
                                    <span class="input-group-addon" id="basic-addon1"><i class="fa fa-user"></i></span>
                                    <input type="text" class="form-control" id="name" name="name" placeholder="Enter your full name" aria-describedby="basic-addon1">
                                </div>
                                <div class="input-group ">
                                    <span class="input-group-addon" id="basic-addon1"><i class="fa fa-mobile"></i></span>
                                    <input type="text" class="form-control mobile" id="mobile" name="mobile" placeholder="Enter your mobile no" aria-describedby="basic-addon1">
                                </div>
                                <div class="input-group ">
                                    <span class="input-group-addon" id="basic-addon1"><i class="fa fa-envelope"></i></span>
                                    <input type="text" class="form-control" id="email" name="email" placeholder="Enter your mobile no" aria-describedby="basic-addon1">
                                </div>
                                <div>
                                    <textarea class="col-md-12 comment" name="content" id="content" placeholder="Enter any comment"></textarea>
                                </div>
                                <p class="text-center"><button class="btn comment col-md-12 label-primary"  type="submit" value="Sign In"><i class="fa fa-send"></i>&nbsp;Send</button></p>
                            </form>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>
                <div id="sms-modal" class="mfp-hide">
                    <div class="login-modal">
                        <div class="booking-info">
                            <h3><a href="{{ path('mobile_home',{'subdomain':globalOption.subDomain}) }}">{{ globalOption.name }}</a></h3>
                        </div>
                        <div class="login-form">
                            <form action="{{ path('webservice_sms_receive',{'subdomain':globalOption.subDomain}) }}" id="send-sms" name="send-sms" method="post">
                                <div class="input-group ">
                                    <span class="input-group-addon" id="basic-addon1"><i class="fa fa-mobile"></i></span>
                                    <input type="text" class="form-control mobile" name="mobile" id="mobile" placeholder="Enter your mobile no" aria-describedby="basic-addon1">
                                </div>
                                <div>
                                    <textarea class="col-md-12 comment sms-content" name="content" maxlength="140" id="content" placeholder="Enter any comment"></textarea>
                                </div>
                                <p class="text-center"><button class="comment btn col-md-12 label-primary"  type="submit" value="Sign In"><i class="fa fa-send"></i>&nbsp;Send</button></p>
                            </form>
                            <div class="clearfix"></div>
                          </div>
                    </div>
                </div>
                <!-- //modal -->
                <div class="footer-bg" id="mobile-footer">
                    <ul class="footer-icon">

                        <li>
                            <a href="callto://088{{ globalOption.mobile }}" class="hvr-radial-out" data-title="Phone">
                                <i area-hidden=true class="fa fa-phone"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#sms-modal" class="hvr-radial-out sign-in popup-top-anim" data-title="">
                                <i class="fa fa-comment"></i>
                            </a>
                        </li>
                        <li>
                            <a href="javascript:" class="scrollToTop" data-title="Top up">
                                <i class="fa fa-hand-o-up"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#email-modal" class="hvr-radial-out sign-in popup-top-anim">
                                <i class="fa fa-envelope"></i>
                            </a>
                        </li>
                        <li>
                            <a class="last hvr-radial-out"
                               href="{{ path('webservice_contact',{"subdomain":globalOption.subDomain}) }}" data-title="">
                                <i class="fa fa-map-marker"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <!--/footer-->

        </div>
    </div>
</div>
{% render controller("FrontendBundle:TemplateWidget:mobileMenu",{'globalOption':globalOption.id}) %}
{% javascripts
'assets/template/mobile/js/jquery-2.1.4.min.js'
'assets/template/mobile/js/bootstrap.min.js'
'assets/template/plugin/jquery-inputmask/jquery.inputmask.bundle.min.js'
'assets/template/plugin/numeric/jquery.numeric.js'
'assets/template/plugin/validation/jquery.validate-1.14.0.min.js'
'assets/template/plugin/validation/jquery-validate.bootstrap-tooltip.min.js'
'assets/template/mobile/js/bigSlide.js'
'assets/template/mobile/js/jquery.magnific-popup.js'
'assets/template/mobile/js/modernizr.custom.js'
'assets/template/mobile/js/jquery.mmenu.min.all.js'
'assets/template/mobile/js/jquery.nicescroll.min.js'
'assets/template/mobile/js/jquery.slidereveal.min.js'
'assets/template/mobile/js/scripts.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>  subdomain = "{{ globalOption.subDomain }}"; </script>
{% block javascriptBlock %}{% endblock %}
<script src="//maps.googleapis.com/maps/api/js?v=3.exp&key=AIzaSyDmior4xLqsriysUeQhtdSawc8yjDyaWaw"></script>
<script type="text/javascript">

    $(document).ready(function () {

        $("#mm-menu").mmenu();
        $('.popup-top-anim').magnificPopup({
            type: 'inline',
            fixedContentPos: false,
            fixedBgPos: true,
            overflowY: 'auto',
            closeBtnInside: true,
            preloader: false,
            midClick: true,
            removalDelay: 300,
            mainClass: 'my-mfp-zoom-in'
        });
        $('.scrollToTop').click(function(){
            $('html, body').animate({scrollTop : 0},800);
            return false;
        });
        $(window).load(function() {
            $('#gmap_canvas').show();
        });
        $(".numeric").numeric();
        $(".mobile").inputmask("mask", {"mask": "99999-999-999"}); //specifying fn & options


        $('.sms-content').keypress(function(e) {

            var tval = $('textarea').val(),
                    tlength = tval.length,
                    set = 140,
                    remain = parseInt(set - tlength);
            $('#limit').text(remain);
            if (remain <= 0 && e.which !== 0 && e.charCode !== 0) {
                $('textarea').val((tval).substring(0, tlength - 1))
            }
        })


        var validator =  $("#send-email").validate({

            rules: {

                "name": {required: true},
                "mobile": {required: true},
                "email": {valid: true},
                "content": {required: true}
            },

            messages: {

                "name":"Enter your full name",
                "mobile":"Enter valid mobile no",
                "content": "Enter your comments"
            },

            tooltip_options: {

                "name": {placement:'bottom',html:true},
                "mobile": {placement:'bottom',html:true},
                "content": {placement:'bottom',html:true}

            },
            submitHandler: function(form) {

                $.ajax({

                    url         : '{{ path('webservice_email_receive',{'subdomain':globalOption.subDomain}) }}',
                    type        : $(form).attr( 'method' ),
                    data        : new FormData(form),
                    processData : false,
                    contentType : false,
                    success: function(response) {
                        $(this).closest('form').find("input[type=text], textarea").val("");
                    },
                    complete: function(){

                    }
                });
            }

        });

        var validator =  $("#send-sms").validate({

            rules: {
                "mobile": {required: true},
                "content": {required: true},
            },

            messages: {
                "mobile":"Enter valid mobile no",
                "content": "Enter your sms text only 160 characters",
            },

            tooltip_options: {
                "mobile": {placement:'bottom',html:true},
                "content": {placement:'bottom',html:true},
            },
            submitHandler: function(form) {

                $.ajax({

                    url         : '{{ path('webservice_sms_receive',{'subdomain':globalOption.subDomain}) }}',
                    type        : $(form).attr( 'method' ),
                    data        : new FormData(form),
                    processData : false,
                    contentType : false,
                    success: function(response) {
                        $(this).closest('form').find("input[type=text], textarea").val("");
                    },
                    complete: function(){

                    }
                });
            }

        });

        $(function() {

            $("#menu-bar a").click(function () {
                var id = $(this).attr("href").substring(1);
                $("html, body").animate({scrollTop: $("#" + id).offset().top}, 1000, function () {
                    $("#menu-bar").slideReveal("hide");
                });
            });

            var slider = $("#side-bar-panel").slideReveal({
                // width: 100,
                push: false,
                position: "right",
                // speed: 600,
                trigger: $(".handle"),
                // autoEscape: false,
                shown: function (obj) {
                    obj.find(".handle").html('<span class="glyphicon glyphicon-chevron-right"></span>');
                    obj.addClass("left-shadow-overlay");
                },
                hidden: function (obj) {
                    obj.find(".handle").html('<span class="glyphicon glyphicon-chevron-left"></span>');
                    obj.removeClass("left-shadow-overlay");
                    $('#registerModal').hide();
                    $('#forgetModal').hide();
                    $('#loginModal').hide();
                    $('#cart-details').hide();
                }
            });
        });

        {% block documentready %}
        {% endblock %}

    });
</script>
<style>
    .modal-body {
        padding:0 !important;
    }
    .panel-body{
        padding:0 !important;
    }

    .box-link{
        height: 45px;
    }
    .social-links {
        padding: 0;
        margin: 0;
        list-style: none;
        padding-right: 5px;
        position: absolute;
        right: 0;
        top: 8px;
    }
    .social-links li {
        display: inline-block;
        margin-left: 5px;
    }
    .social-links li a {
        display: block;
        width: 30px;
        height: 30px;
        line-height: 30px;
        border-radius: 50%;
        font-size: 17px;
        color: #f9eee8;
        border: 1px solid #f9eee8;
        transition: all 0.5s ease 0s;
        text-align: center;
    }

    .help-block{
        margin-top: 0;
    }
    #cat-search{
        margin-bottom:15px;
        clear: both;
    }

    ul.cart-list{
        list-style: none;
        padding-top: 15px;
    }
    ul.cart-list li{
        list-style: none;
        margin-left: 15px;
        padding-bottom: 15px;
        border-bottom: 1px solid #FFFFff;
        margin-bottom: 15px;
        display:inline-block;
    }
    .cart-product-title{
        font-size: 15px;
        padding-bottom: 15px;
    }

    #side-bar-panel{
           position: fixed;
           width: 300px!important;
           transition: all 300ms ease;
           height: 100%;
           top: 0px;
           right: 0px;
       }

    #side-bar-panel{
        background-color: #BDBDBD;
        color: #fff;
        z-index: 1002;
    }
    .left-shadow-overlay{
        box-shadow: 10px 0 15px 10px #585858;
    }

    #side-bar-panel .handle{
        background-color: #2E2E2E;
        left: -40px;
        padding: 10px;
        position: absolute;
        top: 2px;
        cursor: pointer;
        width: 40px!important;
        -moz-transition: 200ms ease all;
        -webkit-transition: 200ms ease all;
        -o-transition: 200ms ease all;
        transition: 200ms ease all;
    }
</style>
</body>
</html>
