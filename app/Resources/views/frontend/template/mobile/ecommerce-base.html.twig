<!DOCTYPE html>
<html lang="en">
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!--> <html lang="en" class="no-js"> <!--<![endif]-->
<head>
    <meta charset="utf-8"/>
    <title>{{ globalOption.name }}</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport"/>
    <meta content="{{ globalOption.domain }}" name="description"/>
    <meta content="{{ globalOption.domain }}" name="author"/>
    <meta name="MobileOptimized" content="320">
    <meta property="og:site_name" content="{{ globalOption.name }}">
    <meta property="og:title" content="{{ globalOption.name }}">
    <meta property="og:description" content="{{ globalOption.domain }}">
    <meta property="og:type" content="website">
    <meta property="og:image" content="{{ image(globalOption.templateCustomize.webPath('logo')).resize() }}"><!-- link to image for socio -->
    <meta property="og:url" content="{{ globalOption.domain }}">
    {% if globalOption.templateCustomize.favicon is not empty %}
        <link  rel="shortcut icon" href="/uploads/domain/{{ globalOption.id }}/customizeTemplate/{{ globalOption.templateCustomize.favicon }}">
    {% endif %}
   {# <script type="text/javascript" src="https://www.google.com/recaptcha/api.js"></script>#}
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyCgg9f-HbB1zWmQY_zsz-vPpYtQd6cA6JA"></script>
    <script type="application/x-javascript"> addEventListener("load", function() {setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    {% stylesheets filter='cssrewrite'
    'assets/template/mobile/css/bootstrap.min.css'
    'assets/template/mobile/css/font-awesome.min.css'
    'assets/plugins/font-awesome/css/font-awesome-4.7.0.min.css'
    'assets/template/mobile/css/jquery.mmenu.all.css'
    'assets/template/plugin/wow/css/animate.css'
    'assets/template/plugin/owl.carousel/owl.carousel.min.css'
    'assets/template/plugin/owl.carousel/owl.theme.min.css'
    'assets/template/plugin/owl.carousel/owl.transitions.css'
    'assets/template/plugin/textillate/assets/animate.css'
    'assets/plugins/pop-modal/popModal.min.css'
    'assets/template/desktop/css/css-loader.css'
    'assets/template/plugin/file-upload/file-upload.css'
    'assets/template/plugin/file-image-uploader/inputFileZone.css'
    'assets/template/desktop/css/module.css'
    'assets/template/mobile/css/style.css'
    'assets/template/desktop/css/socialicon.css'
    %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
    <style>
        .bodyImageLoad{
            background: #009b3a;
            z-index: 999999999;
            display:    none;
            position:   fixed;
            top:        0;
            left:       0;
            height:     100%;
            width:      100%;
            background-image: url("{{ asset('assets/img/loading/preloader.gif') }}")
            50% 50%
            no-repeat;
        }
    </style>
    <!-- web-fonts -->
    <link href='//fonts.googleapis.com/css?family=Abril+Fatface' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
    <!-- //web-fonts -->
    {{ include('frontend/template/mobile/templateCustomize.html.twig')}}
</head>
<body>
{% block cssBlock %}{% endblock %}
<div class="body-back siteBgColor">
    <div class="masthead pdng-stn1">
        <div class="phone-box wrap push" id="home">
            <div class="menu-notify sticky">
                <div class="bar-left">
                    <a href="#mm-menu" class="main-menu"><i class="fa fa-navicon"></i></a>
                </div>
                <div class="bar-mid">
                    <h5 class="pro-link"><a class="site-name" href="{{ path(globalOption.subDomain ~ '_ecommercesubdomain') }}">{{ globalOption.name }}</a></h5>
                </div>
                <div class="bar-right">
                    <a href="javascript:" style="position: relative"  class="showCartItem bar-icon"><i class="fa fa-shopping-bag"></i>
                    <span id="item-qnt" class="totalItem">{{ cart.contents|length }}</span>
                    </a>
                    <a href="javascript:" class="bar-icon upload-pres" id=""><i class="fa fa-camera"></i></a>
                    {% if app.user is not empty and is_granted('ROLE_CUSTOMER') == true %}
                        {% set name = app.user.profile.name|split(" ") %}
                        <a href="{{ path('domain_customer_homepage',{'shop':globalOption.slug}) }}"><i class="fa fa-home"></i></a>&nbsp;&nbsp;<a href="/logout"> <i class="fa fa-sign-out"></i></a>
                    {% elseif app.user is not empty %}
                        {% set name = app.user.profile.name|split(" ") %}
                        <a href="{{ path('homepage') }}"> {{ name[0] }}</a>
                    {% else %}
                    <a href="#login-modal" class="popup-top-anim bar-icon" ><i class="fa fa-lock"></i></a>
                    {% endif %}
                </div>
                <div class="clearfix"></div>
            </div>
            <!-- banner -->
            {% block body %}{% endblock %}
            <!--/footer-->
            <div class="w3agile alt-mobile-footer">
                {{ include('@Frontend/Template/Mobile/Widget/smsEmail.html.twig')}}
                {% if globalOption.templateCustomize.mobileFooter == 1 %}
                <div class="footer-bg" id="mobile-footer">
                    <ul class="footer-icon">

                        <li>
                            <a href="tel://88{{ globalOption.hotline }}" class="hvr-radial-out" data-title="Phone">
                                <i area-hidden=true class="fa fa-phone"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#login-modal" class="hvr-radial-out sign-in popup-top-anim" data-title="">
                                <i class="fa fa-comment"></i>
                            </a>
                        </li>
                        <li>
                            <a href="javascript:" class="scrollToTop" data-title="Top up">
                                <i class="fa fa-hand-o-up"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#email-modal" class="hvr-radial-out sign-in popup-top-anim">
                                <i class="fa fa-envelope"></i>
                            </a>
                        </li>
                        <li>
                            <a class="last hvr-radial-out"
                               href="{{ path(globalOption.subDomain ~ '_webservice_contact') }}" data-title="">
                                <i class="fa fa-map-marker"></i>
                            </a>
                        </li>
                    </ul>
                </div>
                {% else %}
                    <div class="container">
                        <div class="copyright-footer">
                            Â© {{ "now"|date('Y') }} <a href="http://www.terminalbd.com">Terminalbd</a> All Rights Reserved.</div>
                    </div>
                {% endif %}
            </div>

            <!--/footer-->
        </div>
    </div>
</div>
<div id="email-confirm" style="display:none; text-align: left!important;">Thank you for your interest in {{ globalOption.domain }}. Your email has been sent successfully to our customer service department.</div>
<div id="email-invalid" style="display:none; text-align: left!important;">Dear Customer, All ready your email/mobile has been send. Thank you for your interest in {{ globalOption.domain }}.</div>
<div id="product-confirm" style="display:none; text-align: left!important;">Thank you for your interest in {{ globalOption.domain }}. Your order has been sent successfully to our customer service department.</div>
{#<div id="mobile-confirm" style="display:none; text-align: left!important;">Thank you for your interest in {{ globalOption.domain }}.<br/><span id="mobile-validate"></span></div>#}
<div id="mobile-confirm" style="display:none; text-align: left!important;">
<span id="mobile-validate"></span></div>
{% render controller("FrontendBundle:EcommerceWidget:mobileMenu",{'globalOption': globalOption.id}) %}
{% javascripts
'assets/template/mobile/js/jquery-2.1.4.min.js'
'assets/template/mobile/js/bootstrap.min.js'
'assets/template/plugin/jquery-inputmask/jquery.inputmask.bundle.min.js'
'assets/template/plugin/numeric/jquery.numeric.js'
'assets/template/plugin/validation/jquery.validate-1.14.0.min.js'
'assets/template/plugin/validation/jquery-validate.bootstrap-tooltip.min.js'
'assets/plugins/jquery.cookie.min.js'
'assets/template/mobile/js/bigSlide.js'
'assets/template/mobile/js/jquery.magnific-popup.js'
'assets/template/mobile/js/modernizr.custom.js'
'assets/template/mobile/js/jquery.mmenu.min.all.js'
'assets/template/mobile/js/jquery.nicescroll.min.js'
'assets/template/mobile/js/jquery.slidereveal.min.js'
'assets/template/plugin/owl.carousel/owl.carousel.min.js'
'assets/template/plugin/matchHeight/height-match.js'
'assets/template/plugin/wow/js/wow.min.js'
'assets/template/plugin/treeview/treeview.js'
'assets/template/plugin/textillate/assets/jquery.fittext.js'
'assets/template/plugin/textillate/assets/jquery.lettering.js'
'assets/template/plugin/file-image-uploader/inputFileZone.js'
'assets/template/plugin/file-upload/file-upload.js'
'assets/plugins/pop-modal/popModal.js'
'assets/template/desktop/js/jquery.scrollbar.min.js'
'assets/template/mobile/js/ecommerce-scripts.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>  subdomain = "{{ globalOption.subDomain }}"; </script>
{% block javascriptBlock %}{% endblock %}
<script type="text/javascript">
    $(document).ready(function () {
        {% block documentready %}{% endblock %}
        $(window).on('load', function() {
        });
    })
</script>

{% if globalOption.templateCustomize.fbMessenger is not empty  %}
    <div class="fb-customerchat"
         page_id="<{{ globalOption.templateCustomize.fbMessenger }}>">
    </div>
{% endif %}

{% if globalOption.templateCustomize.tawk is not empty  %}
    <script type="text/javascript">
        var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
        (function(){
            var s1=document.createElement("script"),s0=document.getElementsByTagName("script")[0];
            s1.async=true;
            s1.src='https://embed.tawk.to/{{ globalOption.templateCustomize.tawk }}/default';
            s1.charset='UTF-8';
            s1.setAttribute('crossorigin','*');
            s0.parentNode.insertBefore(s1,s0);
        })();
    </script>
{% endif %}

{% if globalOption.templateCustomize.liveChat is not empty  %}
    <script type="text/javascript">function add_chatinline(){var hccid={{ globalOption.templateCustomize.liveChat }};var nt=document.createElement("script");nt.async=true;nt.src="https://mylivechat.com/chatinline.aspx?hccid="+hccid;var ct=document.getElementsByTagName("script")[0];ct.parentNode.insertBefore(nt,ct);}
        add_chatinline(); </script>
{% endif %}

{% if globalOption.templateCustomize.powr is not empty  %}
    <div class="powr-chat" id="{{ globalOption.templateCustomize.powr }}"></div><script src="https://www.powr.io/powr.js?platform=html"></script>
{% endif %}
<script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());
    gtag('config', 'UA-111963509-1');
</script>
</body>
</html>


