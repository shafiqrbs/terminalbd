{% extends '::base.html.twig' %}

{% block body %}
    <div class="container-fluid">
        <!-- BEGIN category HEADER-->
        <div class="row-fluid">
            <div class="span12">
                <h3 class="category-title">
                    Patient ID #  <strong>{{ entity.invoice }}</strong>
                </h3>
                <ul class="breadcrumb">
                    <li>
                        <i class="icon-home"></i>
                        <a href="{{ path('homepage') }}">Home</a>
                        <span class="icon-angle-right"></span>
                    </li>
                    <li>
                        <a href="{{ path('dms_invoice') }}">Patient Invoice</a>
                        <span class="icon-angle-right"></span>
                    </li>
                    <li><a href="#">Manage Patient Invoice</a></li>
                    <li class="pull-right btn blue disabled mini">
                        <i class="fa fa-calendar"></i>
                        {{ "now"|date("d-F-Y") }}
                    </li>
                </ul>
            </div>
        </div>
        <!-- END category HEADER-->
        <!-- BEGIN category CONTENT-->
        <div class="row-fluid">
            <div class="span12">

                <div class="portlet box green tabbable">
                    <div class="portlet-title">
                        <div class="caption"><i class="icon-reorder"></i>Patient Information</div>
                        <div class="actions">
                            <div class="btn-group">
                                <a class="btn" href="{{ path('dms_invoice') }}"><i class="icon-th-list"></i> Patient Lists</a>
                                <a class="btn red" href="{{ path('dms_invoice_new') }}"><i class="icon-plus-sign"></i> Add New Patient</a>
                            </div>
                        </div>
                    </div>
                    <div class="loader loader-double" ></div>
                    <div class="portlet-body">
                        <div class="portlet-body form">
                            {% include('flash-message.html.twig')  %}
                            <!-- BEGIN FORM-->
                            {{ form_start(form) }}
                            {{ form_row(form._token) }}
                            <div class="row-fluid">
                                <div class="span12">
                                    <!--BEGIN TABS-->
                                    <div class="tabbable tabbable-custom tabs-left">
                                        <!-- Only required for left/right tabs -->
                                        <ul class="nav nav-tabs tabs-left">
                                            <li class=""><a href="#tab_patient_overview" data-toggle="tab">Patient Overview</a></li>
                                            <li class="active"><a href="#tab_patient_info" data-toggle="tab">Patient Information</a></li>
                                            {% if entity.customer is not empty %}
                                            <li class=""><a href="#tab_patient_chief" data-toggle="tab">Chief Complaints</a></li>
                                            <li class=""><a href="#tab_patient_presenting" data-toggle="tab">Presenting Complaints</a></li>
                                            <li class=""><a href="#tab_patient_past" data-toggle="tab">Past Medical History</a></li>
                                            <li class=""><a href="#tab_patient_drug" data-toggle="tab">Drug History</a></li>
                                            <li class=""><a href="#tab_patient_general" data-toggle="tab">General Examination</a></li>
                                            <li class=""><a href="#tab_patient_local" data-toggle="tab">Local Examination</a></li>
                                            <li class=""><a href="#tab_patient_investigation" data-toggle="tab">Investigation</a></li>
                                            <li class=""><a href="#tab_patient_diagnosis" data-toggle="tab">Diagnosis</a></li>
                                            <li class=""><a href="#tab_patient_advise" data-toggle="tab">Advice</a></li>
                                            <li class=""><a href="#tab_patient_special_advise" data-toggle="tab">Special Advice</a></li>
                                        {% endif %}
                                        </ul>
                                        <div class="tab-content">
                                            <div class="tab-pane tab-height" id="tab_patient_overview" >
                                                <div class="form-actions portlet box blue">
                                                    <div class="portlet-title">
                                                        <div class="caption"><i class="icon-reorder"></i>PATIENT PROFILE</div>
                                                    </div>
                                                </div>
                                                <div class="clearfix">&nbsp;</div>
                                                <div class="row-fluid" >
                                                    <div class="span12">
                                                        <div class="row-fluid">
                                                            <div class="span8 profile-info">
                                                                {% if entity.customer is not empty %}<h1>{{ entity.customer.name }}</h1>{% endif %}
                                                                <ul class="unstyled inline">
                                                                    {% if entity.customer is not empty and entity.customer.location is not empty %}<li><i class="icon-map-marker"></i>{{ entity.customer.location.id }}</li>{% endif %}
                                                                    <li><i class="icon-calendar"></i> {{ entity.created|date('d-F-Y') }}</li>
                                                                    {% if entity.customer is not empty %}<li><i class="icon-phone"></i> {{ entity.customer.mobile }}</li>{% endif %}
                                                                </ul>
                                                            </div>
                                                            <!--end span8-->
                                                            <div class="span4">
                                                                <div class="portlet sale-summary">
                                                                    <div class="portlet-title">
                                                                        <div class="caption">Profile</div>
                                                                    </div>
                                                                    <div class="portlet-body">
                                                                        <ul class="unstyled">
                                                                            <li>
                                                                                <span class="sale-info">Gender <i class="icon-img-up"></i></span>
                                                                                <span class="sale-num"> {% if entity.customer is not empty %} {{ entity.customer.gender }}{% endif %}</span>
                                                                            </li>
                                                                            <li>
                                                                                <span class="sale-info">Age <i class="icon-img-down"></i></span>
                                                                                <span class="sale-num">{% if entity.customer is not empty %} {{ entity.customer.age }} {{ entity.customer.ageType }} {% endif %}</span>
                                                                            </li>

                                                                        </ul>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <!--end span4-->
                                                        </div>
                                                        <!--end row-fluid-->
                                                        <div class="clearfix">&nbsp;</div>
                                                        <div class="tabbable tabbable-custom tabbable-custom-profile">
                                                            <ul class="nav nav-tabs">
                                                                <li class="active"><a href="#tab_1_11" data-toggle="tab">Chief & Presenting</a></li>
                                                                <li><a href="#tab_1_22" data-toggle="tab">Medical & Drug</a></li>
                                                                <li><a href="#tab_1_33" data-toggle="tab">Examination</a></li>
                                                                <li><a href="#tab_1_44" data-toggle="tab">Investigation & Diagnosis</a></li>
                                                                <li><a href="#tab_1_55" data-toggle="tab">Advise</a></li>
                                                            </ul>
                                                            <div class="tab-content">
                                                                <div class="tab-pane active" id="tab_1_11">
                                                                    <div class="portlet-body" style="display: block;">
                                                                        <div class="form-actions portlet box blue">
                                                                            <div class="portlet-title">
                                                                                <div class="caption"><i class="icon-reorder"></i>CHIEF COMPLAINTS</div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="clearfix">&nbsp;</div>
                                                                        <table class="table table-striped table-bordered table-advance table-hover">
                                                                            <thead class="flip-content ">
                                                                            <tr class="head-green">
                                                                                <th class="numeric">Procedure</th>
                                                                                <th class="numeric">Teeth Position</th>
                                                                                <th class="numeric">Action</th>
                                                                            </tr>
                                                                            </thead>
                                                                            <tbody id="procedure-chief-complaints">
                                                                            {% for invoiceParticular in entity.invoiceParticulars %}
                                                                                {% if invoiceParticular.dmsService is not empty and invoiceParticular.dmsService.slug|trim =='chief-complaints' %}
                                                                                    <tr id="remove-{{ invoiceParticular.id }}">
                                                                                        <td  class="numeric">{{ invoiceParticular.metaValue }}</td>
                                                                                        <td class="numeric">
                                                                                            {% if invoiceParticular.teethPosition is not empty %}
                                                                                                <table class="dms-table">
                                                                                                    {% set leftTeeths = [8,7,6,5,4,3,2,1] %}
                                                                                                    {% set rightTeeths = [1,2,3,4,5,6,7,8] %}
                                                                                                    <tr><td class="dms-td dms-td-border-none dms-td-border-bottom">
                                                                                                            <ul class="leftTeeth">
                                                                                                                {% for left in leftTeeths %}
                                                                                                                    <li {% if left in invoiceParticular.teethNo and invoiceParticular.teethPosition == 'upper-left' %} class="active" {% endif %} >{{ left }}</li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </td>
                                                                                                        <td class="dms-td dms-td-border-bottom">
                                                                                                            <ul class="rightTeeth">
                                                                                                                {% for right in rightTeeths %}
                                                                                                                    <li {% if right in invoiceParticular.teethNo and invoiceParticular.teethPosition == 'upper-right' %} class="active" {% endif %} >{{ right }}</li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                    <tr><td class="dms-td dms-td-border-none">
                                                                                                            <ul class="leftTeeth">
                                                                                                                {% for left in leftTeeths %}
                                                                                                                    <li {% if left in invoiceParticular.teethNo and invoiceParticular.teethPosition == 'lower-left' %} class="active" {% endif %}>{{ left }}</li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </td>
                                                                                                        <td class="dms-td">
                                                                                                            <ul class="rightTeeth">
                                                                                                                {% for right in rightTeeths %}
                                                                                                                    <li {% if right in invoiceParticular.teethNo and invoiceParticular.teethPosition == 'lower-right' %} class="active" {% endif %} >{{ right }}</li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                </table>
                                                                                            {% endif %}
                                                                                        </td>
                                                                                        <td class="numeric">
                                                                                            <a href="javascript:" class="btn red mini particularDetete" data-id="{{ invoiceParticular.id }}" id="{{ invoiceParticular.id }}" data-url="{{ path('dms_invoice_particular_delete',{'invoice':entity.invoice , 'id':invoiceParticular.id}) }}" ><i class="icon-trash"></i></a>
                                                                                        </td>
                                                                                    </tr>
                                                                                {% endif %}
                                                                            {% endfor %}
                                                                            </tbody>
                                                                        </table>
                                                                        <div class="form-actions portlet box green">
                                                                            <div class="portlet-title">
                                                                                <div class="caption"><i class="icon-reorder"></i>HISTORY OF THE PRESENTING COMPLAINTS</div>
                                                                            </div>
                                                                        </div>
                                                                        <div class="clearfix">&nbsp;</div>
                                                                        <table class="table table-striped table-bordered table-advance table-hover">
                                                                            <thead class="flip-content ">
                                                                            <tr class="head-green">
                                                                                <th class="numeric">Procedure</th>
                                                                                <th class="numeric">Teeth Position</th>
                                                                                <th class="numeric">Action</th>
                                                                            </tr>
                                                                            </thead>
                                                                            <tbody id="procedure-chief-complaints">
                                                                            {% for invoiceParticular in entity.invoiceParticulars %}
                                                                                {% if invoiceParticular.dmsService is not empty and invoiceParticular.dmsService.slug|trim =='presenting' %}
                                                                                    <tr id="remove-{{ invoiceParticular.id }}">
                                                                                        <td  class="numeric">{{ invoiceParticular.metaValue }}</td>
                                                                                        <td class="numeric">
                                                                                            {% if invoiceParticular.teethPosition is not empty %}
                                                                                                <table class="dms-table">
                                                                                                    {% set leftTeeths = [8,7,6,5,4,3,2,1] %}
                                                                                                    {% set rightTeeths = [1,2,3,4,5,6,7,8] %}
                                                                                                    <tr><td class="dms-td dms-td-border-none dms-td-border-bottom">
                                                                                                            <ul class="leftTeeth">
                                                                                                                {% for left in leftTeeths %}
                                                                                                                    <li {% if left in invoiceParticular.teethNo and invoiceParticular.teethPosition == 'upper-left' %} class="active" {% endif %} >{{ left }}</li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </td>
                                                                                                        <td class="dms-td dms-td-border-bottom">
                                                                                                            <ul class="rightTeeth">
                                                                                                                {% for right in rightTeeths %}
                                                                                                                    <li {% if right in invoiceParticular.teethNo and invoiceParticular.teethPosition == 'upper-right' %} class="active" {% endif %} >{{ right }}</li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                    <tr><td class="dms-td dms-td-border-none">
                                                                                                            <ul class="leftTeeth">
                                                                                                                {% for left in leftTeeths %}
                                                                                                                    <li {% if left in invoiceParticular.teethNo and invoiceParticular.teethPosition == 'lower-left' %} class="active" {% endif %}>{{ left }}</li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </td>
                                                                                                        <td class="dms-td">
                                                                                                            <ul class="rightTeeth">
                                                                                                                {% for right in rightTeeths %}
                                                                                                                    <li {% if right in invoiceParticular.teethNo and invoiceParticular.teethPosition == 'lower-right' %} class="active" {% endif %} >{{ right }}</li>
                                                                                                                {% endfor %}
                                                                                                            </ul>
                                                                                                        </td>
                                                                                                    </tr>
                                                                                                </table>
                                                                                            {% endif %}
                                                                                        </td>
                                                                                        <td class="numeric">
                                                                                            <a href="javascript:" class="btn red mini particularDetete" data-id="{{ invoiceParticular.id }}" id="{{ invoiceParticular.id }}" data-url="{{ path('dms_invoice_particular_delete',{'invoice':entity.invoice , 'id':invoiceParticular.id}) }}" ><i class="icon-trash"></i></a>
                                                                                        </td>
                                                                                    </tr>
                                                                                {% endif %}
                                                                            {% endfor %}
                                                                            </tbody>
                                                                        </table>

                                                                    </div>
                                                                </div>
                                                                <!--tab-pane-->
                                                                <div class="tab-pane" id="tab_1_22">
                                                                    <div class="form-actions portlet box green">
                                                                        <div class="portlet-title">
                                                                            <div class="caption"><i class="icon-reorder"></i>Past Medical History</div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="clearfix">&nbsp;</div>
                                                                    <ul class="feeds">
                                                                        {% for particular in particulars %}
                                                                            {% if particular.service.slug == 'medical-history' %}
                                                                                <li> <div class="col1">
                                                                                        <div class="cont">
                                                                                            <div class="cont-col2">
                                                                                                <div class="desc">{{ particular.name }}: {% if invoiceParticularArr is not empty and invoiceParticularArr[particular.id] is defined and invoiceParticularArr[particular.id] is not empty %}{{ invoiceParticularArr[particular.id].metaValue }}{% endif %}</div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </li>
                                                                            {% endif %}
                                                                        {% endfor %}
                                                                    </ul>
                                                                    <div class="clearfix">&nbsp;</div>
                                                                    <div class="form-actions portlet box green">
                                                                        <div class="portlet-title">
                                                                            <div class="caption"><i class="icon-reorder"></i>Drug History</div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="clearfix">&nbsp;</div>
                                                                    <div class="row-fluid">
                                                                        <div class="span12">{{ entity.drugHistory|raw }}</div>
                                                                    </div>
                                                                    <div class="clearfix">&nbsp;</div>
                                                                </div>
                                                                <div class="tab-pane" id="tab_1_33">
                                                                    <div class="form-actions portlet box green">
                                                                        <div class="portlet-title">
                                                                            <div class="caption"><i class="icon-reorder"></i>General Examination</div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="clearfix">&nbsp;</div>
                                                                    <ul class="feeds">
                                                                        {% for particular in particulars %}
                                                                            {% if particular.service.slug == 'general' %}
                                                                                <li> <div class="col1">
                                                                                        <div class="cont">
                                                                                            <div class="cont-col2">
                                                                                                <div class="desc">{{ particular.name }}: {% if invoiceParticularArr is not empty and invoiceParticularArr[particular.id] is defined and invoiceParticularArr[particular.id] is not empty %}{{ invoiceParticularArr[particular.id].metaValue }}{% endif %}</div>
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </li>
                                                                            {% endif %}
                                                                        {% endfor %}
                                                                    </ul>
                                                                    <div class="clearfix">&nbsp;</div>
                                                                    <div class="form-actions portlet box green">
                                                                        <div class="portlet-title">
                                                                            <div class="caption"><i class="icon-reorder"></i>Local Examination</div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="clearfix">&nbsp;</div>
                                                                    {% for particular in particulars %}
                                                                        {% if particular.service.slug == 'physical' %}
                                                                            <div class="row-fluid">
                                                                                <div class="span12"><label>{{ particular.name }}</label></div>
                                                                            </div>
                                                                            <div class="row-fluid">
                                                                                <div class="span12">
                                                                                    {% if invoiceParticularArr is not empty and invoiceParticularArr[particular.id] is defined and invoiceParticularArr[particular.id] is not empty %}{{ invoiceParticularArr[particular.id].metaValue|raw }}{% endif %}
                                                                                </div>
                                                                            </div>
                                                                            <div class="clearfix">&nbsp;</div>
                                                                        {% endif %}
                                                                    {% endfor %}
                                                                    <div class="clearfix">&nbsp;</div>
                                                                </div>
                                                                <!--tab-pane-->
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end span9-->
                                                </div>
                                                <div class="clearfix">&nbsp;</div>
                                            </div>
                                            <div class="tab-pane active tab-height" id="tab_patient_info">
                                                <div class="form-actions portlet box green">
                                                    <div class="portlet-title">
                                                        <div class="caption"><i class="icon-user"></i>PATIENT INFORMATION</div>
                                                    </div>
                                                </div>
                                                <div class="clearfix">&nbsp;</div>
                                                <p>ADDING PATIENT INFORMATION</p>
                                                <div class="row-fluid">
                                                    <div class="span12 patient-info">
                                                        <div class="row-fluid customer-add">
                                                            <div class="span4">
                                                                <div class="control-group">
                                                                    <label class="control-label red" for="firstName"><span>Patient Name</span><span class="required">*</span></label>
                                                                    <div class="controls">
                                                                        {{ form_widget(form.customer.name) }}
                                                                        {{ form_errors(form.customer.name) }}
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="span4">
                                                                <div class="control-group">
                                                                    <label class="control-label red" for="firstName"><span>Mobile no</span><span class="required">*</span></label>
                                                                    <div class="controls">
                                                                        {{ form_widget(form.customer.mobile) }}
                                                                        {{ form_errors(form.customer.mobile) }}
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="span4">
                                                                <div class="row-fluid">
                                                                    <div class="span12">
                                                                        <div class="control-group">
                                                                            <label class="control-label red" for="firstName"><span>Gender & Age</span></label>
                                                                            <div class="controls span12">
                                                                                {{ form_widget(form.customer.gender) }}
                                                                            </div>

                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row-fluid">
                                                                    <div class="span12">
                                                                        <div class="control-group">
                                                                            <label class="control-label red" for="firstName"><span>Age</span><span class="required">*</span></label>
                                                                            <div class="controls span4">
                                                                                {{ form_widget(form.customer.ageGroup) }}
                                                                            </div>
                                                                            <div class="controls span4">
                                                                                {{ form_widget(form.customer.age) }}
                                                                            </div>
                                                                            <div class="controls span4">
                                                                                {{ form_widget(form.customer.ageType) }}
                                                                                {{ form_errors(form.customer.ageType) }}
                                                                            </div>
                                                                            <div class="clearfix"></div>
                                                                            <div class="controls">
                                                                                {{ form_errors(form.customer.age) }}
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row-fluid">
                                                                    <div class="span12">
                                                                        <div class="control-group">
                                                                            <label class="control-label red" for="firstName"><span>Location</span></label>
                                                                            <div class="controls">
                                                                                {{ form_widget(form.customer.location) }}
                                                                                {{ form_errors(form.customer.location) }}
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row-fluid">
                                                                    <div class="span12">
                                                                        <div class="control-group">
                                                                            <label class="control-label red" for="firstName"><span>Address</span></label>
                                                                            <div class="controls">
                                                                                {{ form_widget(form.customer.address) }}
                                                                                {{ form_errors(form.customer.address) }}
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="clearfix">&nbsp;</div>

                                                <div class="form-actions portlet box grey">
                                                    <div class="pull-right">
                                                        <button type="submit"  class="btn blue span12" id="savePatientButton"><i class="icon-save"></i> Save & Continue</button>
                                                    </div>
                                                </div>
                                            </div>
                                            {% if entity.customer is not empty %}
                                            <div class="tab-pane tab-height" id="tab_patient_chief" >
                                                <div class="form-actions portlet box blue">
                                                    <div class="portlet-title">
                                                        <div class="caption"><i class="icon-user"></i>CHIEF COMPLAINTS</div>
                                                    </div>
                                                </div>
                                                <div class="clearfix">&nbsp;</div>
                                                <div class="clearfix">&nbsp;</div>
                                                <div class="row-fluid" id="">
