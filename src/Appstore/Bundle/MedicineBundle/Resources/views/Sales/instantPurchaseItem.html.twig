<div class="clearfix">&nbsp;</div>
<form name="instantPurchase" method="post" action="{{ path('medicine_instantpurchase_item_add') }}" class="form-horizontal" id="instantPurchase" novalidate="novalidate">
    <div class="row-fluid particular-info">
        <div class="span4">
            <input type="text" id="vendor" name="vendor" required="required" class="m-wrap span12 input select2Vendor" placeholder="Search vendor name" aria-required="true">
        </div>
        <div class="span2">
            <input type="text" id="purchasesBy" name="purchasesBy" required="required" class="m-wrap span12 input select2User" placeholder="Search received user name" aria-required="true">
        </div>
        <div class="span5">
            <input type="text" id="medicineName" name="medicineName" required="required" class="m-wrap span12 input select2StockMedicine" placeholder="Search medicine name" aria-required="true">
        </div>
        <div class="span1">
            <input type="text" id="purchasePrice" name="purchasePrice" required="required" class="m-wrap span12 input" placeholder="Purchase price" aria-required="true">
        </div>

    </div>
    <div class="row-fluid particular-info">
        <div class="span6">&nbsp;</div>
        <div class="input-append span6 pull-right">
            <div class="input-group">
                <input type="text" id="expirationStartDate" name="expirationStartDate" required="required" class="m-wrap span3 input" placeholder="Expiry start date" aria-required="true">
                <input type="text" id="expirationEndDate" name="expirationEndDate" required="required" class="m-wrap span3 input" placeholder="Expiry end date" aria-required="true">
                <input type="text" id="purchaseQuantity" name="purchaseQuantity" required="required" class="m-wrap span3 form-control input-number input" placeholder="Purchase qnt" aria-required="true">
                <input type="text" id="salesQuantity" name="salesQuantity"  class="m-wrap span2 form-control input-number input" placeholder="Sales qnt" aria-required="true">
                <button type="submit" class="btn green" id="addInstantPurchase" data-url=""> <span class="fa fa-save"></span> Add</button>
            </div>
        </div>
    </div>
</form>
<div class="clearfix">&nbsp;</div>
<style>
    .td-input{
        height: 25px!important;
        padding: 0!important;
        min-height: 25px!important;
    }
    .ui-autocomplete{
        z-index: 99999!important;
    }
    .ul.ui-autocomplete{
        background-color:#4d90fe!important;
    }
</style>