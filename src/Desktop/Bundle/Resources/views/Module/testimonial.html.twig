<div class="page-header-clear"></div>
    <div class="content">
        <div class="container">
            <h1>Testimonial</h1>

            <div class="col-md-12">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-xs-12">
                                <ul id="newsFeedsss">

                                    {% for  row in entity.testimonials %}

                                        <li><div class="one-responsive">

                                                <div class="big-notification dark-notification">
                                                    <img class="responsive-image" src="{{ image(row.webPath) }}">
                                                    <h4>{{ row.name }} <br/>
                                                        <em>{{ row.designation }}</em>, {{ row.title }}
                                                        <br/><em>{{ row.created|date('F j, Y') }}</em>
                                                    </h4>
                                                    <p>{{ row.content|raw }}</p>
                                                </div>
                                            </div></li>
                                    {% endfor %}

                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="panel-footer"> </div>

                </div>
            </div>
            <div class="decoration"></div>
        </div>
    </div>
