{% extends ':frontend:base.html.twig' %}
{% block body %}

    <div id="tm-page-body">
    <div id="tm-template-default-fullwidth" class="tm-container">
    <div class="page-content">
    <div class="page-content-inner tm-content">
    <div class="tm-row" style="padding:50px 0 0 0">
        <div class="tm-wrap">
            <div class="tm-gallery gallery-slide flexslider" style="">
                <ul class="slides">
                    {% for slide in sliders %}
                        <li>
                            <a href="javascript:"><img alt="img" src="{{ image(slide.webPath) }} "></a>
                            <p class="slider-title">{{ slide.title }}</p>
                        </li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>

    <div class="tm-row" style="padding:50px 0 20px 0">
        <div class="tm-wrap">
            <div class="tm-iconbox tm-style1">
                {% for feature in siteContent %}
                    {% set parent = feature.parent.id|default(0) %}
                    {% if( parent == 5 ) %}
                        <div class="one-fourth tm-box">
                    <div class="iconlist-icon-header">
					    <span>
                            {% if loop.index == 1 %}
                            <i class="fa fa-trophy"></i>
                            {% endif %}
                            {% if loop.index == 2 %}
                            <i class="fa fa-flash"></i>
                            {% endif %}
                            {% if loop.index == 3 %}
                            <i class="fa fa-signal"></i>
                            {% endif %}
                        </span>
                        <h3 class="tm-title">{{ feature.name }}</h3>
                    </div>
                    <div class="iconlist-entry">
                        <div class="iconlist-content">
                            <p>
                                {{ feature.content|raw }}
                            </p>
                        </div>
                    </div>
                </div>
                    {% endif %}
                {% endfor %}
                {% for feature in siteContent %}
                    {% if(feature.slug == 'recent-admission') %}
                        <div class="one-fourth tm-box last">
                        <div class="iconlist-icon-header">
                            <span><i class="fa fa-signal"></i></span>
                            <h3 class="tm-title">{{ feature.name }}</h3>
                        </div>
                        <div class="textwidget">
                            <img alt="img"  src="{{ image(feature.webPath).resize(300,140) }}">
                        </div>

                    </div>
                    {% endif %}
                {% endfor %}
            </div>
        </div>
    </div>

    <div class="tm-row">
        <div class="tm-wrap">
            <div style="padding:30px;background:#29739D;margin:30px 0">
                <div class="two-third">
                    <p style="color:#fff">
                        Fedora is a design-led property development & interior design company that specializes in creating the finest residential addresses based in United States and more than 50 countries all over the world.
                    </p>
                </div>
                <div class="last one-third">
                    <div style="text-align:right" class="">
                        <a href="#" class="tm-btn blue">See More</a>
                        <a href="{{ path('contactpage') }}" class="tm-btn green">Contact Us</a>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
        </div>
    </div>

