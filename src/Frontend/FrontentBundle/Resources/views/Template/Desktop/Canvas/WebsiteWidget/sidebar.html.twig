{% if entities is defined and entities is not empty  and widget.module.slug == 'faq' %}
    <div class="blog-sidebar">
        {% if  widget.moduleFeatureName is not empty %}
            <div class="page-title-bar">
                {% if  widget.moduleFeatureName is not empty %}
                    <div class="col-md-12" >
