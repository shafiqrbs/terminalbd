<div class="clearfix"></div>
{% if widget.page is not empty %}
<div class="blog-sidebar">
    {% if  widget.pageFeatureName is not empty or widget.pageShowType == 'slider' %}
        <div class="page-title-bar">
            {% if  widget.pageFeatureName is not empty %}
                <div class="col-md-9" >
                    <div class="page-title"><i class="fa fa-book"></i>&nbsp;{{ widget.pageFeatureName }}</div>
                </div>
            {% endif %}
            {% if widget.pageShowType == 'slider' %}
                <div class="col-md-3 text-right owl-position" >
                    <a class='feature-page-prev' href='javascript:'>
                        <span class='glyphicon glyphicon-chevron-left'></span>
                    </a>
                    <a class='feature-page-next' href='javascript:'>
                        <span class='glyphicon glyphicon-chevron-right'></span>
                    </a>
                </div>
            {% endif %}
        </div>
    {% endif %}
    {% if widget.pageShowType == 'grid' %}
        <div class="row">
            {% for entity in widget.page %}
                <div class="col-md-12 wow fadeInDown grid" data-wow-delay="2s"  >
                    <div class="sidebar-blog card">
                        <div class="post-img">
                            <img class="img-responsive" src="{{ image(entity.webPath).scaleResize(320,200) }}" alt="">
                            <div class="over-layer">
                                <ul class="post-link">
                                    <li><a href="{{  path(globalOption.subDomain ~ '_webservice_module',{'module': entity.slug }) }}" class="fa fa-link"></a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="post-review">
                            <h5 class="post-title"><a href="{{  path(globalOption.subDomain ~ '_webservice_module',{'module':entity.slug }) }}">{{ entity.name }}</a></h5>
                            <p class="post-description">
                                {{ entity.content|truncate(120)|raw }}
                            </p>
                            <a href="{{  path(globalOption.subDomain ~ '_webservice_module',{'module':entity.slug }) }}" class="read-more">read more</a>
                        </div>
                    </div>
                </div>
            {% endfor %}
        </div>
    {% elseif widget.pageShowType == 'slider' %}
        <div class="row">
            <div id="feature-sidebar-slider" class="owl-carousel">
                {% for entity in  widget.page  %}
                    <div class="col-md-12 wow fadeInDown" data-wow-delay="2s" >
                        <div class="sidebar-blog">
                            <div class="post-img">
                                <img src="{{ image(entity.webPath).resize(320,200) }}" alt="">
                                <div class="over-layer">
                                    <ul class="post-link">
                                        <li><a href="{{  path(globalOption.subDomain ~ '_webservice_module',{'module': entity.slug }) }}" class="fa fa-link"></a></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="post-review">
                                <h5 class="post-title"><a href="{{  path(globalOption.subDomain ~ '_webservice_module',{'module':entity.slug }) }}">{{ entity.name }}</a></h5>
                                <p class="post-description">
                                    {{ entity.content|truncate(120)|raw }}
                                </p>
                                <a href="{{  path(globalOption.subDomain ~ '_webservice_module',{'module':entity.slug }) }}" class="read-more">read more</a>
                            </div>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>
    {% else  %}
        <div class="col-md-12">
            <ul class="list-line list-group">
                {% for entity in  widget.page  %}
                    <li><a href="{{ path(globalOption.subDomain ~ '_webservice_module',{'module':entity.slug }) }}">{{ entity.name }}</a></li>
                {% endfor %}
            </ul>
        </div>
    {% endif  %}
    <div class="clearfix"></div>
</div>
{% endif %}
