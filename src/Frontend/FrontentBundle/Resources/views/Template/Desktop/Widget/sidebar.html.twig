{% if entities is defined and entities is not empty  and module.slug == 'event' %}
    <div class="blog-sidebar">
        <h4 class="sidebar-title"><i class="fa fa-align-left"></i>&nbsp;Events</h4>
        <div class="media">
            <div class="row">
                <div class="col-md-12">
                    <div id="sidebar-event-slider" class="owl-carousel">
                        {% for entity in entities  %}
                            <div class="event-sidebar">
                                <div class="post-img">
                                    <img src="{{ image(entity.webPath).resize() }}" alt="" class="img-responsive">
                                    <a href="{{ path('webservice_module_page',{'subdomain': globalOption.subDomain,'module': 'event','slug':entity.slug }) }}" class="overlay"><i class="fa fa-plus-square-o"></i></a>
                                </div>

                                <div class="post-review">
                                    <ul class="post-comment">
                                        <li>{{ entity.created|date('d') }}<small>{{ entity.created|date('M') }}</small></li>
                                        <li><i class="fa fa-map-marker"></i><small>{{ entity.location.name }}</small></li>
                                    </ul>
                                    <h5 class="post-title"><a href="{{ path('webservice_module_page',{'subdomain': globalOption.subDomain,'module': 'event','slug':entity.slug }) }}">{{ entity.name }}</a></h5>
                                    <p class="post-description">{{ entity.content|truncate(120)|raw }}</p>
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endif %}
{% if entities is defined and entities is not empty  and module.slug == 'faq' %}
    <div class="blog-sidebar">
        <h4 class="sidebar-title"><i class="fa fa-align-left"></i>&nbsp;FAQ</h4>
        <div class="media">
            <div class="row">
                <div class="col-md-12">
                    <div class="panel-group" id="sidebar-accordion" role="tablist" aria-multiselectable="true">
                        {% for key,entity in entities  %}
                            <div class="panel panel-default">
                                <div class="panel-heading" role="tab" id="heading{{ key  }}">
                                    <h4 class="panel-title">
                                        <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse{{ key  }}" aria-expanded="{% if key == 0 %}true{% else %}false{% endif %}" aria-controls="collapse{{ key  }}">
                                            {{ entity.name }}
                                        </a>
                                    </h4>
                                </div>
                                <div id="collapse{{ key  }}" class="panel-collapse collapse {% if key == 0 %}in{% endif %}" role="tabpanel" aria-labelledby="heading{{ key  }}">
                                    <div class="panel-body">
                                        <p>  {{ entity.content|raw }}</p>
                                    </div>
                                </div>
                            </div>
                        {% endfor %}
                    </div>
                </div>
            </div>
        </div>

    </div>
{% endif %}
