<div class="bannerPane bannerPane-business-locaton">
    <div class="overlay"></div>
    <div class="s-banner-content">
        <div><img width="100" src="{{ image(globalOption.templateCustomize.webPath('logo')).resize() }}" /></div>
        {{ moduleName }}
    </div>
</div>
<div id="content">
    {% for details in pagination %}
        <article>
            <a href="{{ path(globalOption.subDomain ~ '_webservice_module_page',{'subdomain': globalOption.subDomain,'module':'noticeboard','slug':details.slug }) }}">
                <p class="posts-summary">
                    {{ details.name }}
                </p>
            </a>
            <div class="a-meta">
                <span><i class="fa fa-calendar"></i>&nbsp;{{ details.startDate|date('F j, Y') }}&nbsp;To&nbsp;{{ details.startDate|date('F j, Y') }}</span>
            </div>
        </article>
    {% endfor %}
</div>
