<div class="bannerPane bannerPane-business-locaton">
    <div class="overlay"></div>
    <div class="s-banner-content">
        <div><img width="100" src="{{ image(globalOption.templateCustomize.webPath('logo')).resize() }}" /></div>
        {{ moduleName }}
    </div>
</div>
<div id="content" class="content-margin">
    {% for  details in globalOption.testimonials %}
        <article>
            <a href="{{ path('webservice_module_page',{'subdomain': globalOption.subDomain,'module': moduleName|lower,'slug':details.slug }) }}">
                <div class="article-img-pane">
                    <img src="{{ image(details.webPath()).resize(136,120) }}" />
                </div>
                <p>{{ details.content|truncate(80)|raw }}</p>
            </a>
            <div class="a-meta">
                <span><i class="fa fa-user"></i>&nbsp;{{ details.name }}
            </div>
        </article>
    {% endfor %}
</div>
