<div class="bannerPane bannerPane-business-locaton" >
    <div class="overlay"></div>
    <div class="s-banner-content">
        <div><img width="100" src="{{ image(globalOption.templateCustomize.webPath('logo')).resize() }}" /></div>
        {{ moduleName }}
    </div>
</div>
<div id="content" class="content-margin">
    <article>
        <div class="module-title">
            <p>{{ details.name }}</p>
            <i class="fa fa-calendar"></i>&nbsp;{{ details.startDate|date('F j, Y') }}&nbsp;&nbsp;To&nbsp;&nbsp;{{ details.endDate|date('F j, Y') }}
        </div>
        <p>{{ details.content|raw }}</p>
        <div class="a-meta">
            <span><i class="fa fa-download"></i>&nbsp;<a target="_blank" href="{#{{ details.webPath }}#}">Download</a></span>
        </div>
    </article>
</div>

